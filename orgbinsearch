#include <iostream>

using namespace std;

int main()
{int a[10],i,n,low,high,mid,found,key;

    cout<<"Enter no of elements\n";
    cin>>n;

    cout<<"Enter array elements in sorted order\n";
    for(i=0;i<n;i++)
            cin>>a[i];
    
    cout<<"The elements of array are:\n";
    for(i=0;i<n;i++)
            cout<<a[i]<<"\t";

    cout<<"\nEnter key to be searched\n";
    cin>>key;

    /* Binary search on sorted array */
    found = 0;
    low=0;
    high=n-1;
while( low <= high)
{
        mid= (low + high) / 2;
        if (key == a[mid]) 
        {
	found = 1; 
	break;
        }
        else if ( key < a[mid] )
	 high = mid - 1;
        else
       	 low = mid + 1;
  }

 if( found == 1)
      cout<<"SUCCESSFUL SEARCH\n";
 else
       cout<<"Search is  	  		FAILED\n";
    



}


TIME OF EXECUTION WAS:0.21 SECONDS
